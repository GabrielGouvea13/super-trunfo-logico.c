#include <stdio.h>

int main() {
    
    char estado1;
    char codigo1[4], nomeCidade1[50];
    float area1, pib1;
    int pontosTuristicos1, populacao1;

    char estado2;
    char codigo2[4], nomeCidade2[50];
    float area2, pib2;
    int pontosTuristicos2, populacao2;

    estado1 = 'A';
    snprintf(codigo1, sizeof(codigo1), "A01");
    snprintf(nomeCidade1, sizeof(nomeCidade1), "Fortaleza");
    populacao1 = 2600000;
    area1 = 313.8f;
    pib1 = 73.436f;
    pontosTuristicos1 = 10;

    printf("\nCarta 1:\n");
    printf("Estado: %c\n", estado1);
    printf("Código: %s\n", codigo1);
    printf("Nome da Cidade: %s\n", nomeCidade1);
    printf("População: %d\n", populacao1);
    printf("Área: %.2f km²\n", area1);
    printf("PIB: %.2f bilhões de reais\n", pib1);
    printf("Número de Pontos Turísticos: %d\n", pontosTuristicos1);

    estado2 = 'B';
    snprintf(codigo2, sizeof(codigo2), "B01");
    snprintf(nomeCidade2, sizeof(nomeCidade2), "Maceio");
    populacao2 = 957916;
    area2 = 509.320f;
    pib2 = 27.484f;
    pontosTuristicos2 = 15;

    printf("\nCarta 2:\n");
    printf("Estado: %c\n", estado2);
    printf("Código: %s\n", codigo2);
    printf("Nome da Cidade: %s\n", nomeCidade2);
    printf("População: %d\n", populacao2);
    printf("Área: %.2f km²\n", area2);
    printf("PIB: %.2f bilhões de reais\n", pib2);
    printf("Número de Pontos Turísticos: %d\n", pontosTuristicos2);


    if (populacao1 > populacao2) {
        printf("\nPopulação: A Carta 1 venceu!\n");
    } else if (populacao1 < populacao2) {
        printf("\nPopulação: A Carta 2 venceu!\n");
    } 
    
    if (area1 > area2) {
        printf("Área: A Carta 1 venceu!\n");
    } else if (area1 < area2) {
        printf("Área: A Carta 2 venceu!\n");
    }

    if (pib1 > pib2) {
        printf("PIB: A Carta 1 venceu!\n");
    } else if (pib1 < pib2) {
        printf("PIB: A Carta 2 venceu!\n");
    }

    if (pontosTuristicos1 > pontosTuristicos2) {
        printf("Pontos Turísticos: A Carta 1 venceu!\n");
    } else if (pontosTuristicos1 < pontosTuristicos2) {
        printf("Pontos Turísticos: A Carta 2 venceu!\n");
    }

    return 0;
}
